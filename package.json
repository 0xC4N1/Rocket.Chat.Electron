{
  "name": "rocketchat",
  "productName": "Rocket.Chat",
  "description": "Rocket.Chat Native Cross-Platform Desktop Application via Electron.",
  "version": "2.15.0-develop",
  "author": "Rocket.Chat Support <support@rocket.chat>",
  "copyright": "Â© 2018, Rocket.Chat",
  "homepage": "https://rocket.chat",
  "license": "MIT",
  "main": "app/background.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/RocketChat/Rocket.Chat.Electron.git"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "watch": "rollup -c --environment BUILD:app -w",
    "electron": "electron .",
    "bundle:app": "rollup -c --environment BUILD:app",
    "bundle:unit": "rollup -c --environment BUILD:unit",
    "bundle:e2e": "rollup -c --environment BUILD:e2e",
    "start": "run-p watch electron",
    "eslint": "eslint src",
    "stylelint": "stylelint src/**/*.less src/**/*.css",
    "lint": "run-s eslint stylelint",
    "pretest": "run-s \"bundle:app --environment NODE_ENV:test\" \"bundle:unit --environment NODE_ENV:test\"",
    "test:main": "xvfb-maybe electron-mocha --require source-map-support/register app/main.specs.js",
    "test:renderer": "xvfb-maybe electron-mocha --require source-map-support/register --renderer app/renderer.specs.js",
    "test": "run-s test:main test:renderer",
    "precoverage": "run-s \"bundle:app --environment NODE_ENV:test\" \"bundle:unit --environment NODE_ENV:test,COVERAGE\"",
    "coverage:main": "xvfb-maybe electron-mocha --require source-map-support/register --reporter tasks/coverage-reporter app/main.specs.js",
    "coverage:renderer": "xvfb-maybe electron-mocha --require source-map-support/register --reporter tasks/coverage-reporter --renderer app/renderer.specs.js",
    "coverage": "run-s coverage:main coverage:renderer",
    "postcoverage": "node tasks/coverage-reporter",
    "pree2e": "run-s \"bundle:app --environment NODE_ENV:test\" \"bundle:e2e --environment NODE_ENV:test\"",
    "e2e": "xvfb-maybe mocha --require source-map-support/register app/e2e.js",
    "changelog": "conventional-changelog --config .github/changelog.js -i HISTORY.md -s",
    "build:darwin": "electron-builder --publish=never --x64 --macos",
    "build:win32": "electron-builder --publish=never --x64 --ia32 --windows",
    "build:linux": "electron-builder --publish=never --x64 --ia32 --linux --c.productName=rocketchat",
    "build:snap": "electron-builder --publish=never --x64 --linux snap --c.productName=rocketchat",
    "build:mas-dev": "electron-builder --publish=never --x64 --mac mas-dev --c.mac.provisioningProfile=Development.provisionprofile",
    "prerelease": "run-s \"bundle:app --environment NODE_ENV:production\"",
    "release": "cross-os build -- --publish=onTagOrDraft"
  },
  "cross-os": {
    "build": {
      "darwin": "run-s \"build:darwin {@}\" --",
      "win32": "run-s \"build:win32 {@}\" --",
      "linux": "run-s \"build:linux {@}\" \"build:snap {@}\" --"
    }
  },
  "dependencies": {
    "@paulcbetts/system-idle-time": "^1.0.4",
    "electron-updater": "^4.0.6",
    "freedesktop-notifications": "^1.4.0",
    "fs-jetpack": "^2.2.0",
    "mem": "^4.0.0",
    "spellchecker": "^3.5.0",
    "tmp": "^0.0.33"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@rocket.chat/eslint-config": "^0.1.2",
    "babel-eslint": "^10.0.1",
    "builtin-modules": "^3.0.0",
    "chai": "^4.2.0",
    "chai-spies": "^1.0.0",
    "conventional-changelog-cli": "^2.0.11",
    "cross-os": "^1.3.0",
    "electron": "^4.0.1",
    "electron-builder": "^20.38.4",
    "electron-mocha": "^6.0.4",
    "eslint": "^5.12.0",
    "eslint-plugin-react": "^7.12.3",
    "gulp": "^3.9.1",
    "gulp-batch": "^1.0.5",
    "gulp-less": "^4.0.1",
    "gulp-plumber": "^1.2.0",
    "gulp-watch": "^5.0.1",
    "minimist": "^1.2.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.1.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "rollup": "^1.1.0",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-copy-assets": "^1.1.0",
    "rollup-plugin-glob-import": "^0.3.1",
    "rollup-plugin-istanbul": "^2.0.1",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-less": "^0.1.5",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "spectron": "^5.0.0",
    "stylelint": "^9.9.0",
    "stylelint-order": "^2.0.0",
    "xvfb-maybe": "^0.2.1"
  },
  "devEngines": {
    "node": ">=8.12.x",
    "npm": ">=4.x",
    "yarn": ">=0.21.3"
  }
}
